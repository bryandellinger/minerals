<div class="card">
    {{> ../../shared/cardheader}}
    <div class="card-body">     
          {{> form}}      
    </div>
</div>

<!-- Modal -->
<div class="modal fade" id="uploadPaymentModal" tabindex="-1" role="dialog" aria-labelledby="uploadPaymentLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="uploadPaymentModalLabel">Cancel</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                Are you sure you want to cancel?
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">No</button>
                <button type="button" id="cancelUploadPaymentChangesBtn" data-dismiss="modal" class="btn btn-primary">Yes</button>
            </div>
        </div>
    </div>
</div>









